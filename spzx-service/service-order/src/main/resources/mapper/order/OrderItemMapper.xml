<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--保证映射文件的命名空间和mapper接口的全类名保持一致-->
<mapper namespace="com.losgai.spzx.order.mapper.OrderItemMapper">


    <insert id="save" useGeneratedKeys="true" keyProperty="id">
        insert into order_item (id,
                                order_id,
                                sku_id,
                                sku_name,
                                thumb_img,
                                sku_price,
                                sku_num)
        values (#{id},
                #{orderId},
                #{skuId},
                #{skuName},
                #{thumbImg},
                #{skuPrice},
                #{skuNum})
    </insert>
</mapper>

